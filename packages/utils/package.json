{
  "name": "@boomerang-io/utils",
  "version": "0.0.1",
  "author": {
    "name": "Tim Bula",
    "email": "timrbula@gmail.com"
  },
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git@github.com:boomerang-io/webapp-packages",
    "directory": "packages/utils"
  },
  "homepage": "https://github.com/boomerang-io/webapp-packages",
  "bugs": {
    "url": "https://github.com/boomerang-io/webapp-packages/issues"
  },
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "npm run build:commonjs",
    "build:commonjs": "cross-env NODE_ENV=production babel ./src --out-dir ./lib --ignore ./**/*.spec.js",
    "lint": "eslint src",
    "prebuild": "rimraf lib",
    "prepare": "npm run-script build",
    "test": "jest --watch",
    "test:ci": "jest --findRelatedTests"
  },
  "dependencies": {
    "axios": "^0.18.1",
    "lodash": "^4.17.15",
    "yup": "^0.28.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@commitlint/cli": "^7.6.1",
    "@commitlint/config-conventional": "^7.6.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "24.7.1",
    "chalk": "2.3.0",
    "cross-env": "5.1.3",
    "eslint": "6.8.0",
    "eslint-plugin-jest": "^22.21.0",
    "extract-text-webpack-plugin": "3.0.2",
    "fs-extra": "5.0.0",
    "html-webpack-plugin": "2.30.1",
    "husky": "^1.3.1",
    "jest": "^24.9.0",
    "lint-staged": "^8.2.1",
    "lodash": "^4.17.15",
    "prettier": "^1.19.1",
    "promise": "8.0.1",
    "rimraf": "2.6.2",
    "sw-precache-webpack-plugin": "0.11.4",
    "webpack": "3.10.0",
    "webpack-dev-server": "2.10.1",
    "webpack-manifest-plugin": "1.3.2"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "gitHead": "34ccc15c5589fe31ec56bfa634b5d6b736a7b143",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "keywords": [
    "boomerang",
    "react",
    "utilities"
  ],
  "lint-staged": {
    "*.{js,jsx,json,scss,css,md}": [
      "prettier --print-width 120 --write"
    ],
    "src/**/*.{js,jsx}": [
      "npm run-script lint",
      "npm run-script test:ci"
    ],
    "*": [
      "git add"
    ]
  },
  "sideEffects": false
}
