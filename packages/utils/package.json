{
  "name": "@boomerang-io/utils",
  "version": "1.0.2",
  "author": {
    "name": "Tim Bula",
    "email": "timrbula@gmail.com"
  },
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git@github.com:boomerang-io/webapp-packages",
    "directory": "packages/utils"
  },
  "homepage": "https://github.com/boomerang-io/webapp-packages",
  "bugs": {
    "url": "https://github.com/boomerang-io/webapp-packages/issues"
  },
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "npm run build:commonjs",
    "build:commonjs": "cross-env NODE_ENV=production babel ./src --out-dir ./lib --ignore ./**/*.test.js",
    "lint": "eslint src",
    "precommit": "lint-staged",
    "prebuild": "rimraf lib es",
    "prepare": "npm run-script build",
    "test": "jest --watch",
    "test:ci": "jest --findRelatedTests"
  },
  "peerDependencies": {
    "axios": ">=0.24.0",
    "lodash": "^4.17.21",
    "query-string": "^6.14.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-query": "^3.34.1",
    "react-router-dom": "^5.3.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "axios": ">=0.24.0",
    "@babel/cli": "^7.16.8",
    "@babel/core": "^7.16.10",
    "@babel/eslint-parser": "7.16.5",
    "@babel/preset-env": "^7.16.11",
    "babel-jest": "^27.4.6",
    "chalk": "^5.0.0",
    "cross-env": "^7.0.3",
    "fs-extra": "^10.0.0",
    "jest": "^27.4.2",
    "lodash": "^4.17.21",
    "promise": "^8.1.0",
    "query-string": "^7.1.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-query": "^3.34.1",
    "react-router-dom": "^5.3.0",
    "rimraf": "2.6.2",
    "yup": "^0.32.11"
  },
  "keywords": [
    "boomerang",
    "react",
    "utilities"
  ],
  "lint-staged": {
    "*.{js,jsx,json,scss,css,md}": [
      "prettier --print-width 120 --write"
    ],
    "src/**/*.{js,jsx}": [
      "npm run-script lint",
      "npm run-script test:ci"
    ],
    "*": [
      "git add"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "sideEffects": false
}
