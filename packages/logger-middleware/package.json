{
  "name": "@boomerang-io/logger-middleware",
  "version": "0.0.1",
  "description": "Boomerang logging middleware for logging consistently on the Boomerang platform",
  "main": "src/index.js",
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "precommit": "lint-staged",
    "test": "jest",
    "test:watch": "nodemon --exec npm test"
  },
  "repository": {
    "type": "git",
    "url": "git@github.ibm.com:Boomerang/boomerang.nodejs.logging.git"
  },
  "author": "Boomerang Engineering",
  "contributors": [
    {
      "name": "Tim Bula",
      "email": "trbula@us.ibm.com"
    }
  ],
  "publishConfig": {
    "registry": "https://tools.boomerangplatform.net/artifactory/api/npm/boomeranglib-npm/"
  },
  "keywords": [
    "boomerang",
    "node",
    "middleware",
    "log4js",
    "logging"
  ],
  "dependencies": {
    "app-root-path": "3.0.0",
    "log4js": "^6.1.2"
  },
  "devDependencies": {
    "express": "^4.16.3",
    "husky": "^4.2.5",
    "jest": "^25.3.0",
    "lint-staged": "^10.0.0",
    "nodemon": "^2.0.0",
    "prettier": "^2.0.0",
    "supertest": "^4.0.0"
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ],
    "src/**/*.{js,jsx}": [
      "npm run-script test"
    ]
  }
}
